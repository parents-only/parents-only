    "@fortawesome/fontawesome-free": "^5.15.2",
    "apollo-server-express": "^2.19.2",
    "if-env": "^1.0.4",
    "react-dom": "^17.0.1",
    "react-ios-switch": "^0.1.19",
    "react-photo-gallery": "^8.0.0",
    "react-tinder-card": "^1.3.1"
  },
  "devDependencies": {
    "concurrently": "^5.2.0"
  }
}
